	Adsurl = {http://adsabs.harvard.edu/abs/2017ApJ...834...27M},
	Archiveprefix = {arXiv},
	Author = {{Minchev}, I. and {Steinmetz}, M. and {Chiappini}, C. and {Martig}, M. and {Anders}, F. and {Matijevic}, G. and {de Jong}, R.~S.},
