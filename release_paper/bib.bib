	Journal = {\aap},
	Keywords = {Galaxy: abundances, Galaxy: evolution, Galaxy: disk, Galaxy: open clusters and associations: general, stars: supernovae: general, Astrophysics - Astrophysics of Galaxies},
	Month = Sep,
	Pages = {87-96},
