	Adsurl = {http://adsabs.harvard.edu/abs/2017A%26A...606A..94D},
	Archiveprefix = {arXiv},
	Author = {{Delgado Mena}, E. and {Tsantaki}, M. and {Adibekyan}, V.~Z. and {Sousa}, S.~G. and {Santos}, N.~C. and {Gonz{\'a}lez Hern{\'a}ndez}, J.~I. and {Israelian}, G.},
	Date-Added = {2018-04-18 15:20:54 +0000},
	Date-Modified = {2018-04-18 15:20:54 +0000},
	Doi = {10.1051/0004-6361/201730535},
	Eid = {A94},
	Eprint = {1705.04349},
	Journal = {\aap},
