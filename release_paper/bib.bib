	Date-Modified = {2018-02-04 20:01:39 +0000},
	Doi = {10.1134/1.163861},
	Journal = {Astronomy Reports},
	Month = jun,
	Pages = {376-388},
	Title = {{Non-LTE Analysis of the Formation of KI Lines in the Spectra of A-K Stars}},
	Volume = 44,
	Year = 2000,
