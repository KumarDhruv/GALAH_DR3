	Doi = {10.1093/mnras/stv2320},
	Eprint = {1510.01376},
	Journal = {\mnras},
	Keywords = {asteroseismology, stars: distances, stars: fundamental parameters, stars: general, Galaxy: disc, Galaxy: evolution},
	Month = jan,
