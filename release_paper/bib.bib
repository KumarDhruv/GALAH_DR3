	Keywords = {stars: kinematics, Galaxy: disc, Galaxy: fundamental parameters, Galaxy: kinematics and dynamics, solar neighbourhood},
	Month = apr,
	Pages = {1829-1833},
	Title = {{Local kinematics and the local standard of rest}},
