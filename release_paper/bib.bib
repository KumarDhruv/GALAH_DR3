	Journal = {The Messenger},
	Keywords = {GaiaESO, GES},
	Month = dec,
	Pages = {47-49},
	Title = {{The Gaia-ESO Large Public Spectroscopic Survey}},
	Volume = 154,
	Year = 2013,
