	Doi = {10.1007/s11214-007-9173-7},
	Journal = {\ssr},
	Keywords = {Sun: abundances, photosphere, corona},
	Month = jun,
	Pages = {105-114},
	Title = {{The Solar Chemical Composition}},
	Volume = 130,
	Year = 2007,
