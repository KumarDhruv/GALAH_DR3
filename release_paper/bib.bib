@article{Amarsi2015,
	Abstract = {{We revisit the Galactic chemical evolution of oxygen, addressing the
systematic errors inherent in classical determinations of the oxygen
abundance that arise from the use of one-dimensional (1D) hydrostatic
model atmospheres and from the assumption of local thermodynamic
equilibrium (LTE). We perform detailed 3D non-LTE radiative-transfer
calculations for atomic oxygen lines across a grid of 3D hydrodynamic
STAGGER model atmospheres for dwarfs and subgiants. We apply our grid of
predicted line strengths of the [O I] 630 nm and O I 777 nm lines using
accurate stellar parameters from the literature. We infer a steep decay
in [O/Fe] for [Fe/H] {\gsim} -1.0, a plateau [O/Fe] {\ap} 0.5 down to
[Fe/H] {\ap} -2.5, and an increasing trend for [Fe/H] {\lsim} -2.5. Our 3D
non-LTE calculations yield overall concordant results from the two
oxygen abundance diagnostics.
}},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2015MNRAS.454L..11A},
	Archiveprefix = {arXiv},
