	Eid = {171},
	Eprint = {1608.08634},
	Journal = {\apj},
	Keywords = {binaries: general, nuclear reactions, nucleosynthesis, abundances, stars: AGB and post-AGB, stars: carbon, stars: chemically peculiar},
	Month = nov,
	Pages = {171},
	Primaryclass = {astro-ph.SR},
