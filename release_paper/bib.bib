	Date-Added = {2018-10-30 15:18:39 +0000},
	Date-Modified = {2018-10-30 15:18:45 +0000},
	Doi = {10.1051/0004-6361/201014483},
	Eid = {A32},
	Eprint = {1006.5863},
	Journal = {\aap},
	Keywords = {Galaxy: abundances, Galaxy: evolution, nuclear reactions, nucleosynthesis, abundances, Astrophysics - Astrophysics of Galaxies},
	Month = Nov,
	Pages = {A32},
	Primaryclass = {astro-ph.GA},
	Title = {{Quantifying the uncertainties of chemical evolution studies. II. Stellar yields}},
	Volume = {522},
	Year = 2010,
