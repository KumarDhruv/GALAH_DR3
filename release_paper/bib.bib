	Doi = {10.1093/mnras/stv2608},
	Eprint = {1511.01155},
	Journal = {\mnras},
	Keywords = {line: formation, radiative transfer, methods: numerical, stars: abundances, stars: atmospheres},
	Month = feb,
	Pages = {3735-3751},
