	Doi = {10.1051/0004-6361:20035959},
	Eprint = {astro-ph/0405198},
	Journal = {\aap},
	Keywords = {Galaxy: disk, Galaxy: solar neighbourhood, Galaxy: stellar content, Galaxy: kinematics and dynamics, Galaxy: evolution, stars: fundamental parameters},
	Month = may,
	Pages = {989-1019},
	Title = {{The Geneva-Copenhagen survey of the Solar neighbourhood. Ages, metallicities, and kinematic properties of {\tilde}14 000 F and G dwarfs}},
