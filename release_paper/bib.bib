	Doi = {10.3847/1538-4357/aa6aff},
	Eid = {67},
	Eprint = {1703.10181},
	Journal = {\apj},
	Keywords = {binaries: spectroscopic, galaxies: clusters: individual: M67, NGC 6791, stars: magnetic field, stars: mass-loss, starspots},
	Month = may,
	Pages = {67},
	Primaryclass = {astro-ph.SR},
	Title = {{On the Origin of Sub-subgiant Stars. II. Binary Mass Transfer, Envelope Stripping, and Magnetic Activity}},
	Volume = 840,
	Year = 2017,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {https://doi.org/10.3847/1538-4357/aa6aff}}

@article{Mishenina2011,
	Abstract = {{We have determined abundances of copper, zinc, sodium, and aluminum in
the atmospheres of 172 F, G, and K dwarf stars (-1.0 $\lt$ [Fe/H] $\lt$
0.3) belonging to the Galaxy's thin and thick disks and to the Hercules
moving group. Our observations were performed with the ELODIE
{\'e}chelle spectrometer on the 1.93-m telescope of the Haute
Provence Observatory, with a resolving power of R = 42 000 and
signal-to-noise ratio S/N $\gt$ 100. The Na, Al, Cu, and Zn abundances
were derived in an LTE approximation; the synthetic spectrum for the
copper lines was calculated taking into account super-fine structure of
the lines. We analyzed the abundances of these elements as a function of
metallicity [Fe/H] for stars of the thin and thick disks of the Galaxy
and the Hercules moving group. The Cu abundances and their trends with
metallicity are essentially the same in the three studied
sub-structures. The mean Al and Zn abundances for stars of the thin and
thick disks differ significantly.
}},
