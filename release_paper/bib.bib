	Adsurl = {http://adsabs.harvard.edu/abs/2016ApJ...823..114N},
	Archiveprefix = {arXiv},
	Author = {{Ness}, M. and {Hogg}, D.~W. and {Rix}, H.-W. and {Martig}, M. and {Pinsonneault}, M.~H. and {Ho}, A.~Y.~Q.},
	Date-Added = {2018-01-21 22:49:01 +0000},
