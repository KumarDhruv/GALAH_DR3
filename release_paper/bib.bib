	Journal = {\aap},
	Keywords = {stars: abundances, stars: fundamental parameters, stars: late-type, Galaxy: evolution, open clusters and associations: general},
	Month = aug,
	Pages = {L8},
	Primaryclass = {astro-ph.SR},
	Title = {{The [Y/Mg] clock works for evolved solar metallicity stars}},
	Volume = 604,
	Year = 2017,
