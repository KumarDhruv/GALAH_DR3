@article{Ting2016,
	Abstract = {{Stars born from the same molecular cloud should be nearly homogeneous in
their element abundances. The concept of chemical tagging is to identify
members of disrupted clusters by their clustering in element abundance
space. Chemical tagging requires large samples of stars with precise
